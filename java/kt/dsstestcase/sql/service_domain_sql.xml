<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="serviceDomain">

	<select id="select" parameterType="java.util.Map" resultType="kt.dsstestcase.vo.serviceDomainVO">
		SELECT sn, service_domain, domain_name 
		FROM testcase_service_domain 
		ORDER BY service_domain
	</select>
	
</mapper>